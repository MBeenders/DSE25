{
  "mission_profile":
  {
    "stages": 2,
    "launch": {"exact": 0},
    "engine1_ignition": {"exact": 0},
    "engine2_ignition": {"delay": 15},
    "separation": {"delay": 0}
  },
  "simulator_parameters":
  {
    "maximum_iterations": 10E5,
    "dt": 0.01
  },
  "plot_selection":
  {
    "mass":
    {
      "x_data": ["iteration"],
      "y_data": ["rocket.mass", "rocket.stage1.mass", "rocket.stage2.mass"],
      "x_label": "Iteration [-]",
      "y_label": "Mass [kg]",
      "x_lim": [0, 40],
      "y_lim": [0, "None"],
      "show_change": "True"
    },
    "length_vs_mass":
    {
      "x_data": ["rocket.length"],
      "y_data": ["rocket.mass"],
      "x_label": "Length [m]",
      "y_label": "Mass [kg]",
      "x_lim": ["None", "None"],
      "y_lim": ["None", "None"],
      "show_change": "False"
    }
  },
  "sizing_selection": ["recovery", "engine", "electronics", "stability"],
  "engine_chemicals":
  {
    "stage1":
    {
      "oxidizer": "example_chemical",
      "fuel": "example_chemical"
    },
    "stage2":
    {
      "oxidizer": "example_chemical",
      "fuel": "example_chemical"
    }
  },
  "export_catia": true,
  "catia_variables":
  {
    "stage1":
    {
      "engine":
      {
        "length": "[m]",
        "diameter": "[m]"
      },
      "recovery":
      {
        "drogue":
        {
          "area": "[m^2]"
        }
      }
    }
  }
}